{
  "name": "SoundCamp",
  "version": "1.15",
  "homepage_url": "http://aelse.github.com/SoundCamp/",
  "update_url": "http://aelse.github.com/SoundCamp/updates.xml",
  "description": "Custom sounds for campfire.",
  "icons": { "128": "images/speaker_128.png" },
  "permissions": [
    "notifications",
    "tabs",
    "*://*.campfirenow.com/room/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.campfirenow.com/room/*"],
      "css": ["soundcamp.css"],
      "js": ["jquery-1.7.1.min.js", "sound.js"]
    }
  ]
}
