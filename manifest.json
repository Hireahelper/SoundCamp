{
  "name": "SoundCamp",
  "version": "1.0",
  "description": "Custom sounds for campfire.",
  "browser_action": {
    "default_icon": "images/speaker.png"
  },
  "permissions": [
    "notifications",
    "tabs",
    "*://*.campfirenow.com/room/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.campfirenow.com/room/*"],
      "css": ["soundcamp.css"],
      "js": ["jquery-1.7.1.min.js", "sound.js"]
    }
  ]
}
